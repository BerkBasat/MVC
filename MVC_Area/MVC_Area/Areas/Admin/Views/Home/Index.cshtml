@model List<MVC_Area.Models.Employee>
@{
    ViewBag.Title = "Index";
}

<div class="card mt-5">
    <div class="card-header">
        <h4>EMPLOYEES</h4>
    </div>
    <div class="card-body">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Employee ID</th>
                    <th>Last Name</th>
                    <th>First Name</th>
                    <th>Title</th>
                    <th>City</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var employee in Model)
                {
                <tr>
                    <td>@employee.EmployeeID</td>
                    <td>@employee.LastName</td>
                    <td>@employee.FirstName</td>
                    <td>@employee.Title</td>
                    <td>@employee.City</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>

